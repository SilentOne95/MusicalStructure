<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstAlbum" android:label="Song List"/>
        <activity android:name=".SecondAlbum" android:label="Song List"/>
        <activity android:name=".ThirdAlbum" android:label="Song List"/>
        <activity android:name=".FourthAlbum" android:label="Song List"/>
        <activity android:name=".FifthAlbum" android:label="Song List"/>
        <activity android:name=".SixthAlbum" android:label="Song List"/>
        <activity android:name=".SeventhAlbum" android:label="Song List"/>
        <activity android:name=".EighthAlbum" android:label="Song List"/>
        <activity android:name=".Player" android:label="Player"/>
    </application>

</manifest>